

<h3>@Model.CurrCategory</h3>

@{
    if (Model.GetAllCars != null )
    {
        <div class="row mt-5 mb-2">
            @{
                foreach (Car car in Model.GetAllCars)
                {
                    <partial name="AllCars" model="car" />
                }
            }
        </div>
    }
    else
    {
        <p>No cars available in this category.</p>
    }
}

